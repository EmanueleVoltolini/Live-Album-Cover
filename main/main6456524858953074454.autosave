int size = 500;
int frame_rate = 60;


PImage img_cover;
ParticleSystem particle_system;

void setup() {
  size(500, 500);
  frameRate(frame_rate);
  
  img_cover = loadImage("cover.jpg");
  imageMode(CENTER);
  noStroke();
  background(0);
  
  //image(img_cover, width/2, height/2);
  particle_system = new ParticleSystem();
  
}

void draw() {
  particle_system.update();
  particle_system.draw();
}
